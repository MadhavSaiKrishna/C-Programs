#include <stdio.h>
struct sist
{
 char Name;
 int Regno;
 float Marks;
}s[5];
int main()
{
 struct sist;
 for(int i=1;i<5;i++)
 {
  printf("Enter the name%d:",i);
  scanf("%s",&s[i].Name);  
  printf("Enter the register number%d:",i);
  scanf("%d ",&s[i].Regno);
  printf("Enter the marks%d:",i);
  scanf("%.1f",&s[i].Marks);
 }
return 0;
}
